set prompt \033[31m(gdb) \033[0m
set disassembly-flavor intel

python
import os
filePath = os.environ['BUILDROOT'] + 'sysroot/kernel.bin'
gdb.execute('file ' + filePath)
end

target remote :1234

define q
monitor quit
end

define reg
monitor info registers
end

define reset
monitor system_reset
end
